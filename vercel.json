{
    "rewrites": [
        {
            "source": "/_next/(.*)",
            "destination": "/_next/$1"
        },
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/icons/(.*)",
            "destination": "/icons/$1"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}